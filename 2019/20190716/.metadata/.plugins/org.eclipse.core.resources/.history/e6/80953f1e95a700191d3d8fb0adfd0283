package pack1_0716;

import java.util.*;
public class src7 {
	public static void main(String args[]) {
		Scanner scan = new Scanner(System.in);
		
		int a = scan.nextInt();
		int b = scan.nextInt();
		
		for(int i=1; i<=a; i++) {
			System.out.print(" ");
		}	for(int j = 1; j<=b; j++) {
				System.out.print("*");
		}
	}
}
