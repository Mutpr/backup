package pack1_0716;

import java.util.*;
public class src7 {
	public static void main(String args[]) {
		Scanner scan = new Scanner(System.in);
		
		int a = scan.nextInt();
		int b = scan.nextInt();
		
		for(int i = 0; i<=a; i++) {
			for(int j = 0; j<=i; j++) 
				System.out.print("*");
				System.out.println("");
		}
	}
}
