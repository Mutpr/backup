/*
 * 사용자로부터 높이와 넓이를 입력받아 중첩된 반복문을 이용해서 다음의 패턴을 출력하시오.
 */
package pack1_0716;

import java.util.*;

public class src6 {
	public static void main(String args[]) {
		Scanner scan = new Scanner(System.in);
		int a;
		a = scan.nextInt();
		int b;
		b= scan.nextInt();
		
		for(int i=1; i<=a; i++) {
			System.out.print("*");
			for(int j=1; j<=b; j++) {
			} System.out.println();
		}
	}
}
