package src_20190802;

import java.util.*;

public class prac0802_2 {
	public static void main(String args[]) {
		double num1;
		double num2;
		
		do {
			num1 = (int)(Math.random()*6);
			num2 = (int)(Math.random()*6);
			if(num1+num2==6) {
				System.out.print("("+num1+","+num2+")");
				break;
			}
		}while(num1+num2!=5);{
			System.out.print("("+num1+","+num2+")");
		}
		}
	}
