package src_20190802;

import java.util.*;

public class prac0802_2 {
	public static void main(String args[]) {
		
		
		while(true){
			
			int num1 = (int)(Math.random()*6)+1;
			int num2 = (int)(Math.random()*6)+1;
			
			if(num1+num2==5) {
				System.out.print("("+num1+","+num2+")");
				break;
			}else{
				System.out.print("("+num1+","+num2+")");
				}
		}
		}
	}
