package src;

import java.util.*;
public class src1 {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		for(int i=0; i<=a; i++) { //별이 출력된 위치
			for(int j=0; j<=b; j++) {
			System.out.println("*");
		}System.out.println();
		}
		
	}

}
