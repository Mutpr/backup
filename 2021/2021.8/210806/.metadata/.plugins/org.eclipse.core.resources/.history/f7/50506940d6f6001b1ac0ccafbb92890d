package src3_210806;

public class MyDetailInfo extends MyInfo{
	String number;
	String univ;
	
	public MyDetailInfo(String name, int age) {
		super(name, age);
		this.name = name;
		this.age = age;
	}
	
	public String setName() {
		return name;
	}
	
	public int age() {
		return age;
	}
	
	public String toString() {
		return super.name+super.age+name+age;
	}
	}
