package src_20200803;

public class Food {
	double cost;
	double weight;
	
	public void SetCost(double cost) {
		this.cost = cost;
	}
	
	public double GetCost() {
		return cost;
	}
	
	public void SetWeight(double weight) {
		this.weight = weight;
	}
	
	public double GetWeight() {
		return cost;
	}
	
}
