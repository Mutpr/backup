package src2_20200927;

public class Shape {
	protected int row;
	protected char alpha;
	
	void Shape(int ro, char ap){
		
	}
	
	public void draw(int x, char ch){
		this.row = x;
		this.alpha = ch;
	}
	
	public int getRow() {
		return row;
	}

	public char getalpha() {
		return alpha;
	}
	
	public String toString() {
		return "";
	}
}
