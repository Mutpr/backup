package src0_20200927;

public class Person {
	protected String name;
	protected String EmpNumber; 
	
	public Person(String na, String Emp) {
		this.name = na;
		this.EmpNumber = Emp;
	}
	
	int computeSalary() {
		return 1;
	}
	
	public String getName() {
		return name;
	}

	public String getEmpNumber() {
		return EmpNumber;
	}
	
	public String toString() {
		return "이름> "+name+"\t"
				+"사번> "+EmpNumber+"\t"
				
	}

}

