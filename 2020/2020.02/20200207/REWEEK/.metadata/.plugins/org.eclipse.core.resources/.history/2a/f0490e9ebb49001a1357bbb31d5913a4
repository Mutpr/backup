package src_20200207_Array;

public class ArrayCreateByValueListExample {
	public static void main(String args[]) {
		int [] scores = {83, 90, 87};
		
		System.out.println();
		System.out.println();
		System.out.println();
		
		int sum = 0;
		for(int i=0; i<3; i++) {
			sum+=scores[i];
		}
		
		System.out.println();
		
	}
}
