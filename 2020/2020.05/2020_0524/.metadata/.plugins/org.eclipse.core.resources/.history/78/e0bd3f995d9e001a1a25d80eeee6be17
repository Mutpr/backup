package src_0524;

public class src1_example {
	public void max (int [] isource) {
		
		int max = -1;
		int min = 100;
		int MaxIndex = 0;
		int MinIndex = 0;

		for(int i=0; i<=isource.length-1; i++) {
				isource[i] = (int)(Math.random()*100)+1;
				if(isource[i]>max) {
					max = isource[i];
					MaxIndex = i+1;
				}
			}
			for(int k = 0; k<isource.length; k++) {
				System.out.print(isource[k]+"\t");
			}System.out.println("\n"+"ÃÖ´ë°ª: "+max+"\t");
			
		}
	}

