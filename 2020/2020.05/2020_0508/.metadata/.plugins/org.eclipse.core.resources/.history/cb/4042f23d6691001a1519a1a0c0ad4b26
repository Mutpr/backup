package src_0508;

import java.util.*;

public class src1 {
	public static void main(String args[]) {
		Scanner scan = new Scanner(System.in);
		
		int max = 1; 
		int value; 
		int [] array1 = new int[9];
		int input;
		
		for(int i =0; i<=array1.length-1; i++) {
			input=scan.nextInt();
			if(input>100) {
				continue;
			}else {
				value = array1[i];
				if(value>max) {
					max = value;
					System.out.println(i);
				}
			}
		}
	}

}
