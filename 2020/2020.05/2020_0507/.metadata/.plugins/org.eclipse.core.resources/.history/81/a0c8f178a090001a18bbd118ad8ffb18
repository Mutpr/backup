package src;

import java.util.*;
public class src1 {
	public static void main(String [] args) {
		Scanner scan = new Scanner(System.in);
		
		int max = -1;
		int min = 1;
		int value;
		
		int length = scan.nextInt();
		int [] array1 = new int [length];
		
		for(int i = 0; i<=length; i++) {
			array1[i] = scan.nextInt();
			
			if(array1[i]<max) {
				continue;
			}else {
				max = array1[i];
				System.out.println(max);
			}
		}	
		
	}
}
