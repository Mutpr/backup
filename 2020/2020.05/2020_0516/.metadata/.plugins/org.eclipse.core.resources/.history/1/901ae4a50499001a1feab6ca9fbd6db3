package src_0516;

import java.util.*;
public class src5 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		boolean [] seat = new boolean[10];
		int menu;
		int seatSel;
		
		System.out.println("메뉴를 선택하세요:");
		System.out.println("1) 대여 2) 대여현황 3) 종료");
		menu = scan.nextInt();
		
		switch(menu) {
		case 1:
			
			for(int i = 0; i<=seat.length; i++) {
				System.out.println("대여하고자 하는 사물함 번호를 입력하세요");
				seatSel = scan.nextInt();
				seat[i] = true;
				break;
			}
		case 2:
		case 3:
		}
		
	}

}
