package src_0516;

import java.util.*;
public class src6 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		
		int [] seat = new int[10];
		int select;
		while(true) {
			select = scan.nextInt();
			if(select == -1) {
				break;
			}else {
				for(int i = 0; i<=seat.length-1; i++) {
					if(seat[i] == select) {
						seat[i] = 1;
						System.out.println(seat[i]);
					}
				}
			}
		}
	}

}
