package src1_20201018;

public class Distance {
	protected String name;
	protected int dist;
	
	public void name() {
	}
	
	public void dist() {
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public int getDist() {
		return dist;
	}
	
	public void setDist(int dist) {
		this.dist = dist;
	}
}
