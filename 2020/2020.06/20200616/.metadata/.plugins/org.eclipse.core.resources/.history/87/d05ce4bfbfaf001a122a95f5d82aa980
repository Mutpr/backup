package src1;

public class Melon extends Food{
	//경작 농원 정보
	public String farm;
	
	public String getFarm() {
		return "경작 농원: "+ farm + "\t,가격: "+ price + "\t, 무게" + weight;
	}
	
	public void setFarm(String farm, int price, int weight) {
		this.farm = farm;
		this.price = price;
		this.weight = weight;
}
	}
