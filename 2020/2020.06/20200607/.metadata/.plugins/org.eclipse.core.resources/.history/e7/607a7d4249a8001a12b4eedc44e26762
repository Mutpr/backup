import java.util.*;
public class wordTest {
	public static void main(String [] args) {
		Scanner scan = new Scanner(System.in);
		word word = new word();
		
		for(int i = 0; i<7; i++) {
			System.out.print("영단어와 뜻을 입력하세요>>");
