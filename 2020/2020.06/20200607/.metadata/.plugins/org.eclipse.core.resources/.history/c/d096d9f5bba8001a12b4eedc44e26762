package src1_20200607;

import java.util.*;
public class WordTest {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		Word [] word;
		word = new Word[4];
		String english;
		String meaning;
		for(int i = 0; i<word.length; i++) {
			english = scan.nextLine();
			meaning = scan.nextLine();
			word [i] = new Word();
			word[i] = getWord(english, meaning);
		}
	}

	}
