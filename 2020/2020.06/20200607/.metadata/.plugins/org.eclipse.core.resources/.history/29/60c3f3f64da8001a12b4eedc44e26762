package src1_20200607;

public class word {
	private String word;
	private String meaning;
	
	private void GetWord(String word){
		this.word = word;
	}
	
	private String SetWord(String word) {
		return word;
	}
	
	private void GetMeaning(String meaning) {
		this.meaning = meaning;
	}
	
	private String SetMeaning(String meaning) {
		return meaning;
	}
	
	public String toString() {
		return word;
	}
	
}
