package src1_20200607;

public class Word {
	private String english;
	private String meaning;
	
	public void setWord(String a, String b) {
		this.english = a;
		this.meaning = b;
	}
	
	public String getWord() {
		return english;
	}
	
	public String getMeaning() {
		return meaning;
	}
	
	
	public String toString() {
		return " "+english+" :"+meaning;
	}
}
