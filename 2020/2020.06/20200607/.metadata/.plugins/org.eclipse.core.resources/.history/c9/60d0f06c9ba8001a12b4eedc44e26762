package src1_20200607;

import java.util.*;
public class wordTest {
	public static void main(String [] args) {
		
		Scanner scan = new Scanner(System.in);
		//word test = new word();
		word[] word = new word[7];
		
		for(int i = 0; i<word.length-1; i++) {
			System.out.println((i+1)+" :영단어와 뜻을 입력하세요>>");
			String english,meaning = scan.nextLine();
			word[i] = new word(english,meaning);
		}
	
	}
}
