package src2_20200608;

public class person {
	private String name;
	private int year;
	
	public void setPerson(String name) {
		this.name =name;
	}
	public String getPerson() {
		System.out.println("이름:"+name);
		return name;
	}
	
	public int getYear(int year) {
		System.out.println("출생년도: "+year);
		return year;
	}
	
	void checkMask() {
		int il = year % 10;
		}
		
	}
